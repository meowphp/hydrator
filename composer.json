{
    "name": "meow/hydrator",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Meow\\Hydrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meow\\Hydrator\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "May Meow",
            "email": "hello@mitsuha.dev",
            "homepage": "https://themaymeow.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.1"
    },
    "scripts": {
        "test": "phpunit tests",
        "cs-check": "phpcs --standard=PSR12 src",
        "cs-fix": "phpcbf --standard=PSR12 src",
        "stan": "phpstan analyse"
    }
}
